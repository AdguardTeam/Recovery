{
  "name": "adblock-recovery",
  "version": "0.1.4",
  "description": "Adblock Recovery extension",
  "scripts": {
    "start": "NODE_ENV=prod ./node_modules/.bin/gulp dev --require babel-register --gulpfile tasks",
    "dev": "./node_modules/.bin/gulp dev --require babel-register --gulpfile tasks && ./node_modules/.bin/gulp watch --require babel-register --gulpfile tasks",
    "ghtest": "./node_modules/.bin/gulp ghtest --require babel-register --gulpfile tasks",
    "locales": "./node_modules/.bin/gulp locales --require babel-register --gulpfile tasks",
    "upload": "./node_modules/.bin/gulp upload --require babel-register --gulpfile tasks",
    "archive": "./node_modules/.bin/gulp archive --require babel-register --gulpfile tasks",
    "test": "mocha-phantomjs test/test.html"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "babelrc": false
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.5.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.9.2",
    "gulp-concat": "^2.6.1",
    "gulp-css2js": "^1.0.2",
    "gulp-download2": "^1.0.2",
    "gulp-file": "^0.4.0",
    "gulp-hash-creator": "^0.2.0",
    "gulp-inline-assets": "^0.1.1",
    "gulp-less": "^3.4.0",
    "gulp-sourcemaps": "^2.6.3",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^4.3.11",
    "gulp-zip": "^4.1.0",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-inline-urls": "^1.2.0",
    "mocha": "^3.5.3",
    "mocha-phantomjs": "^4.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-fs": "^3.0.2",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "readability": "git+https://github.com/AdguardTeam/readability.git"
  }
}
